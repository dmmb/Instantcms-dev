RewriteEngine on

RewriteEngine On
RewriteBase /
RewriteRule ^.htaccess$ - [F]

RewriteCond $1 !^(index\.php|(.*)view=search(.*)|images(.*)|(components|modules|templates)(.*)images(.*)|robots\.txt|(.*)\.jpg|(.*)\.gif|(.*)\.xml|(.*)\.png|(.*)\.swf|(.*)\.js|(.*)\.css|admin|(.*)fckeditor(.*)|(.*).php)

RewriteRule ^(.*)$ /index.php?uri=$1 [L]